package kr.or.ddit.vo;

public class ReviewVO {
	private int rev_id;
	private String tit;
	private String cont;
	private String ins_dt;
	private int star;
	private int prod_id;
	private String buyer_id;
	private String file_id;
	
	// 기본 생성자
	public ReviewVO() {}
	
	// 생성자
	public ReviewVO(int rev_id, String tit, String cont, String ins_dt, int star, int prod_id, String buyer_id,
			String file_id) {
		super();
		this.rev_id = rev_id;
		this.tit = tit;
		this.cont = cont;
		this.ins_dt = ins_dt;
		this.star = star;
		this.prod_id = prod_id;
		this.buyer_id = buyer_id;
		this.file_id = file_id;
	}

	// getter, setter
	public int getRev_id() {
		return rev_id;
	}

	public void setRev_id(int rev_id) {
		this.rev_id = rev_id;
	}

	public String getTit() {
		return tit;
	}

	public void setTit(String tit) {
		this.tit = tit;
	}

	public String getCont() {
		return cont;
	}

	public void setCont(String cont) {
		this.cont = cont;
	}

	public String getIns_dt() {
		return ins_dt;
	}

	public void setIns_dt(String ins_dt) {
		this.ins_dt = ins_dt;
	}

	public int getStar() {
		return star;
	}

	public void setStar(int star) {
		this.star = star;
	}

	public int getProd_id() {
		return prod_id;
	}

	public void setProd_id(int prod_id) {
		this.prod_id = prod_id;
	}

	public String getBuyer_id() {
		return buyer_id;
	}

	public void setBuyer_id(String buyer_id) {
		this.buyer_id = buyer_id;
	}

	public String getFile_id() {
		return file_id;
	}

	public void setFile_id(String file_id) {
		this.file_id = file_id;
	}

	@Override
	public String toString() {
		return "ReviewVO [rev_id=" + rev_id + ", tit=" + tit + ", cont=" + cont + ", ins_dt=" + ins_dt + ", star="
				+ star + ", prod_id=" + prod_id + ", buyer_id=" + buyer_id + ", file_id=" + file_id + "]";
	}
	
}
